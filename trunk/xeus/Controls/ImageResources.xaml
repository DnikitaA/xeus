<ControlTemplate x:Key="MessageIcon" TargetType="{x:Type Button}">
	<ControlTemplate.Resources>
		<Storyboard x:Key="PumpMessageForever">
			<ObjectAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="viewbox" Storyboard.TargetProperty="(UIElement.Visibility)"
				RepeatBehavior="Forever" AutoReverse="True" >
				<DiscreteObjectKeyFrame KeyTime="00:00:00" Value="{x:Static Visibility.Visible}"/>
				<DiscreteObjectKeyFrame KeyTime="00:00:00.3000000" Value="{x:Static Visibility.Hidden}"/>
				<DiscreteObjectKeyFrame KeyTime="00:00:00.6000000" Value="{x:Static Visibility.Visible}"/>
