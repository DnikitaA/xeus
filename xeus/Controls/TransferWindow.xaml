<win:WindowBase
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  	xmlns:local="clr-namespace:xeus.Core"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:win="clr-namespace:xeus.Controls"
    xmlns:settings="clr-namespace:xeus.Core"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2006" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
    x:Class="xeus.Controls.TransferWindow"
    Title="file transfer" Width="383"
    settings:WindowSettings.Save="True"
	Style="{StaticResource DarkStyleWindow}"
    Icon="../images/xeus.ico"
    x:Name="filetransfer"
	Background="{x:Null}" ResizeMode="NoResize" Margin="5,5,5,5" ShowInTaskbar="True" VerticalAlignment="Top" SizeToContent="WidthAndHeight" HorizontalAlignment="Left" >
	<StackPanel Margin="2,2,2,2" Orientation="Horizontal">
    <win:FileTransfer x:Name="_transfer" HorizontalAlignment="Left" VerticalAlignment="Top"/>
  </StackPanel>
</win:WindowBase>
