<Storyboard x:Key="PumpStatus">
	<ThicknessAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="viewbox" Storyboard.TargetProperty="(FrameworkElement.Margin)">
		<SplineThicknessKeyFrame KeyTime="00:00:00" Value="10,10,10,10"/>
		<SplineThicknessKeyFrame KeyTime="00:00:00.2000000" Value="-2,-2,-2,-2"/>
		<SplineThicknessKeyFrame KeyTime="00:00:00.4000000" Value="0,0,0,0"/>
	</ThicknessAnimationUsingKeyFrames>
</Storyboard>

<Storyboard x:Key="PumpStatusDown">
	<ThicknessAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="viewbox" Storyboard.TargetProperty="(FrameworkElement.Margin)">
		<SplineThicknessKeyFrame KeyTime="00:00:00" Value="0,0,0,0"/>
		<SplineThicknessKeyFrame KeyTime="00:00:00.2000000" Value="-2,-2,-2,-2"/>
		<SplineThicknessKeyFrame KeyTime="00:00:00.4000000" Value="10,10,10,10"/>
	</ThicknessAnimationUsingKeyFrames>
</Storyboard>

<Storyboard x:Key="BlinkMessage">
	<ThicknessAnimationUsingKeyFrames RepeatBehavior="Forever" AutoReverse="True" 
		BeginTime="00:00:00" Storyboard.TargetName="viewbox" Storyboard.TargetProperty="(FrameworkElement.Margin)">
		<SplineThicknessKeyFrame KeyTime="00:00:00" Value="10,10,10,10"/>
		<SplineThicknessKeyFrame KeyTime="00:00:00.2000000" Value="-2,-2,-2,-2"/>
		<SplineThicknessKeyFrame KeyTime="00:00:00.4000000" Value="0,0,0,0"/>
	</ThicknessAnimationUsingKeyFrames>
</Storyboard>

