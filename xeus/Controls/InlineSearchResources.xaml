<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:Microsoft_Windows_Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Classic">
	<!-- Resource dictionary entries should be defined here. -->
	<Style x:Key="TextBoxStyleInline" TargetType="{x:Type TextBox}">
		<Setter Property="FontSize" Value="14"/>
		<Setter Property="Background" Value="#00BBBBBB"/>
		<Setter Property="Foreground" Value="#FFDADADA"/>
		<Setter Property="BorderBrush" Value="#00B7B7B7"/>
	</Style>
</ResourceDictionary>